{
  "name": "parallel-rc",
  "description": "Simply run multiple shell commands in parallel.",
  "author": "Chris Ackerman <chris@topher.land>",
  "license": "ISC",
  "version": "1.0.11",
  "files": [
    "lib/**"
  ],
  "directories": {
    "lib": "lib"
  },
  "type": "module",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "bin": {
    "parallel-rc": "lib/bin.js",
    "rc": "lib/bin.js"
  },
  "sideEffects": false,
  "scripts": {
    "test": "del-cli out && tsc --noEmit && eslint --max-warnings=0 src && jest --no-cache --passWithNoTests",
    "prepack": "del-cli lib *.tgz && tsc --project tsconfig.build.json && chmodx --package",
    "start": "npm run prepack && node lib/bin.js",
    "modernize": "ncu -i --format group"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.5",
    "chmodx": "^1.0.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.22.0",
    "eslint-config-rational": "^1.0.2",
    "jest": "^28.1.3",
    "npm-check-updates": "^16.0.5",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "cliopts": "^1.0.1",
    "cross-spawn": "^7.0.3",
    "supports-color": "^9.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shakeskeyboarde/parallel-rc.git"
  },
  "keywords": [
    "shell",
    "bash",
    "cmd",
    "concurrent",
    "concurrently",
    "parallel",
    "command",
    "commands",
    "sh",
    "run",
    "exec",
    "cli",
    "execute",
    "spawn"
  ]
}
